    "test": "npm run lint && npm run security-check && npm run coverage"
  },
  "dependencies": {},
  "devDependencies": {
    "chai": "^4.0.0",
    "coffee-coverage": "^2.0.0",
    "coffeelint": "^2.1.0",
    "coffeescript": "^2.4.1",
    "husky": "0.13.0",
    "lodash": "^4.17.15",
    "mocha": "^6.2.2",
    "nyc": "^15.0.0"
  },
  "peerDependencies": {
    "coffeelint": "^2.1.0"
  },
  "coffeelintConfig": {
    "extends": "../../../lib/index.coffee"
  }
}
